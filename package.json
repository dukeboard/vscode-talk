{
    "name": "vscode-talk",
    "displayName": "vscode-talk",
    "description": "",
    "version": "0.0.3",
    "publisher": "dukeboard",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dukeboard/vscode-talk.git"
    },
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/extension",
    "contributes": {
        "markdown.markdownItPlugins": true,
        "markdown.previewStyles": [
            "./screen-4x3.css",
            "./fonts/roboto_light_macroman/stylesheet.css",
            "./fontawesome/css/fontawesome-all.css",
            "./agate.css"
        ],
        "markdown.previewScripts": [
            "./preview.js"
        ],
        "commands": [
            {
                "command": "markdown.extension.printToHtml",
                "title": "Markdown: Print current document to HTML",
                "category": "Print",
                "icon": {
                    "light": "./media/icon-light.svg",
                    "dark": "./media/icon-dark.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "editorLangId == markdown",
                    "command": "markdown.extension.printToHtml",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "3.0.3",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "fs": "0.0.1-security",
        "highlightjs": "^9.12.0",
        "markdown-it": "^8.4.2",
        "markdown-it-decorate": "^1.2.2",
        "markdown-it-named-headers": "0.0.4",
        "markdown-it-regexp": "^0.4.0",
        "markdown-it-sub": "^1.0.0",
        "markdown-it-sup": "^1.0.0"
    }
}
