{
    "name": "vscode-talk",
    "displayName": "vscode-talk",
    "description": "",
    "version": "0.0.2",
    "publisher": "dukeboard",
    "config": {
        "puppeteer_skip_chromium_download": true
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dukeboard/vscode-talk.git"
    },
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/extension",
    "contributes": {
        "markdown.markdownItPlugins": true,
        "markdown.previewStyles": [
            "./screen-4x3.css",
            "./fonts/roboto_light_macroman/stylesheet.css",
            "./fontawesome/css/fontawesome-all.css",
            "./agate.css"
        ],
        "markdown.previewScripts": [
            "./preview.js"
        ],
        "commands": [
            {
                "command": "markdown.extension.printToHtml",
                "title": "Markdown: Print current document to HTML",
                "category": "Print",
                "icon": {
                    "light": "./media/html.svg",
                    "dark": "./media/html.svg"
                }
            },
            {
                "command": "markdown.extension.printToPdf",
                "title": "Markdown: Print current document to PDF",
                "category": "Print",
                "icon": {
                    "light": "./media/pdf.svg",
                    "dark": "./media/pdf.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "editorLangId == markdown",
                    "command": "markdown.extension.printToPdf",
                    "group": "navigation"
                },
                {
                    "when": "editorLangId == markdown",
                    "command": "markdown.extension.printToHtml",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "fs": "0.0.1-security",
        "markdown-it-decorate": "^1.2.2",
        "markdown-it-regexp": "^0.4.0",
        "markdown-it-sub": "^1.0.0",
        "markdown-it-sup": "^1.0.0",
        "puppeteer": "^1.5.0"
    }
}
